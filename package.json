    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "babel-loader": "8.3.0",
    "copyfiles": "2.4.1",
    "css-loader": "5.2.6",
    "rimraf": "4.1.2",
    "sass": "1.58.1",
    "sass-loader": "10.1.1",
